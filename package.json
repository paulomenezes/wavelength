		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"framer-motion": "^10.16.4",
		"graphql-request": "^7.1.2",
		"lucide-react": "^0.487.0",
		"next": "15.3.0-canary.41",
		"node-vibrant": "^4.0.3",
