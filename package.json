	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"dev": "next dev --turbo --port 3001",
		"preview": "next build && next start",
		"start": "next start",
		"tsc": "tsc --noEmit",
		"codegen": "pnpm run codegen:graphql && pnpm run codegen:database",
		"codegen:graphql": "graphql-codegen --config codegen.ts",
		"codegen:database": "supabase gen types typescript --project-id xjnlpzknrprldnstlfsi > src/supabase/database.types.ts",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"db:migrate:up": "supabase migration up --linked",
